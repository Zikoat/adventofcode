{
  "dependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/bun": "^1.3.3",
    "@typescript/native-preview": "^7.0.0-dev.20260105.1",
    "temporal-polyfill": "^0.3.0",
    "zod": "^4.1.13"
  },
  "scripts": {
    "debug": "bun --inspect-brk d12",
    "type": "tsgo --noEmit",
    "type:watch": "tsgo --noEmit --watch",
    "test": "bun test",
    "test:watch": "bun test --watch --reporter=dots",
    "format": "biome check --write",
    "format:unsafe": "biome check --write --unsafe",
    "validate": "bun i && tsgo --noEmit && bun test --coverage --coverage-reporter=lcov --only-failures && bun run format",
    "bigboy": "bun run validate && bun run d12-bigboy",
    "watch": "bun run watch.ts"
  }
}
